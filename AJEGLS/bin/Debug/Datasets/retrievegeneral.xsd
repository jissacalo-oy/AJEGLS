UNION ALL
SELECT        dbo.historytbl.datee, dbo.charttbl.chartname, dbo.historytbl.debit, dbo.historytbl.credit, dbo.historytbl.remarks, dbo.historytbl.journalcategory, dbo.suppliertbl.companyname,''
